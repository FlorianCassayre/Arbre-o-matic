    "bootstrap": "^4.3.1",
    "bootstrap-colorpicker": "^3.1.2",
    "bootstrap-select": "^1.13.10",
    "d3": "^5.11.0",
    "jquery": "^3.4.0",
    "panzoom": "^8.1.2",
    "parse-gedcom": "^1.0.5",
    "pdfkit": "^0.10.0",
    "popper.js": "^1.15.0",
    "seedrandom": "^3.0.3",
    "svg-to-pdfkit": "^0.1.7"
  },
