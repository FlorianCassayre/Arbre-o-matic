{
  "name": "arbre-o-matic",
  "version": "0.2.0",
  "description": "Genealogical fan generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianCassayre/Arbre-o-matic.git"
  },
  "keywords": [
    "gedcom",
    "genealogy",
    "visualization"
  ],
